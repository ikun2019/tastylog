<div class="card">
  <a href="/shops/<%= item.id %>" class="text-body">
    <img class="card-img-top" src="/public/images/dummy/286x180.png" />
    <div class="card-body">
      <h5 class="card-title"><%= item.name%></h5>
      <p class="card-text"><%= item.categories %></p>
      <p class="card-text"><span class="rate-star" style="--rate: <%= (item.score/5.00)*100 %>%;"></span> <%= item.score %></p>
    </div>
  </a>
</div>